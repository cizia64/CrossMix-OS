<?xml version="1.0" ?>
<!DOCTYPE msxconfig SYSTEM 'msxconfig2.dtd'>
<msxconfig>
  <info>
    <name>MSX-AUDIO</name>
    <manufacturer/>
    <code/>
    <release_year>1986</release_year>
    <description>Generic Yamaha Y8950 MSX-AUDIO, including MIDI ports to enable proper detection.</description>
    <type>sound expansion</type>
  </info>
  <devices>
    <MSX-AUDIO id="Generic MSX-Audio">
      <io base="0xC0" num="2"/>
      <io base="0x0A" num="1" type="O"/>
      <type>Philips</type>
      <sound>
        <volume>12000</volume>
      </sound>
      <sampleram>256</sampleram>
    </MSX-AUDIO>

    <MusicModuleMIDI id="Generic MSX-Audio-MIDI">
      <io base="0x00" num="2" type="O"/>
      <io base="0x04" num="2" type="I"/>
    </MusicModuleMIDI>
  </devices>
</msxconfig>
