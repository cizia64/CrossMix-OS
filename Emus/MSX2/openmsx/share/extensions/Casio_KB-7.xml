<?xml version="1.0" ?>
<!DOCTYPE msxconfig SYSTEM 'msxconfig2.dtd'>
<msxconfig>
  <info>
    <name>Docking station</name>
    <manufacturer>Casio</manufacturer>
    <code>KB-7</code>
    <release_year>1984</release_year>
    <description>Docking station for the Casio PV-7 with 2 slots, additional 8KB RAM and a printer port</description>
    <type>docking station</type>
  </info>

  <devices>

    <PrinterPort id="KB-7 Printer Port">
      <io base="0x90" num="2"/>
    </PrinterPort>

    <primary slot="0">
      <RAM id="KB-7 RAM">
        <mem base="0xC000" size="0x2000"/>
      </RAM>
    </primary>

    <primary external="true" slot="3"/>
    <primary external="true" slot="2"/>

  </devices>

</msxconfig>
