<?xml version="1.0" ?>
<!DOCTYPE msxconfig SYSTEM 'msxconfig2.dtd'>
<msxconfig>
  <info>
    <name>Slot Expander</name>
    <manufacturer/>
    <code/>
    <release_year/>
    <description>Generic 4x slot expander. Creates 4 secondary slots in the primary slot it has been placed in.</description>
    <type>slot expander</type>
  </info>
  <devices>
    <primary slot="any">
      <secondary external="true" slot="0"/>
      <secondary external="true" slot="1"/>
      <secondary external="true" slot="2"/>
      <secondary external="true" slot="3"/>
    </primary>
  </devices>
</msxconfig>
